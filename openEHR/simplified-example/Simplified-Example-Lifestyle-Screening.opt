                <term_definitions code="at0000">
                    <items id="text">Fysisk aktivitet</items>
                    <items id="description">En summering av den typiska nivån av fysisk aktivitet som en person ligger på.</items>
                </term_definitions>
                <term_definitions code="at0001">
                    <items id="text">Tree</items>
                    <items id="description">@ internal @</items>
                </term_definitions>
                <term_definitions code="at0002">
                    <items id="text">Fysisk aktivitetsnivå (PAL)</items>
                    <items id="description">Fysisk aktivitetsnivå</items>
                    <items id="comment">Kodning med terminologi är önskvärt där det är möjligt.</items>
                </term_definitions>
                <term_definitions code="at0003">
                    <items id="text">Beskrivning</items>
                    <items id="description">En beskrivning av graden av fysisk aktivitet.</items>
                </term_definitions>
                <term_definitions code="at0005">
                    <items id="text">Hinder</items>
                    <items id="description">Identifierade faktorer som hindrar personen från att bli mer fysiskt aktiv.</items>
                </term_definitions>
                <term_definitions code="at0006">
                    <items id="text">Möjliggörare</items>
                    <items id="description">Identifierade faktorer som kan hjälpa personen till att bli mer fysiskt aktiv.</items>
                    <items id="comment">Till exempel: bor nära en simbassäng; eller tidigare en elitidrottare.</items>
                </term_definitions>
                <term_definitions code="at0007">
                    <items id="text">Fysisk träning</items>
                    <items id="description">Normal varaktighet för mycket aktivitet.</items>
                </term_definitions>
                <term_definitions code="at0008">
                    <items id="text">Vardagsmotion</items>
                    <items id="description">Normal varaktighet för medelmåttig aktivitet.</items>
                </term_definitions>
                <term_definitions code="at0009">
                    <items id="text">Extremt inaktiv</items>
                    <items id="description">Personen är extremt inaktiv, till exempel en sängliggande patient.</items>
                </term_definitions>
                <term_definitions code="at0010">
                    <items id="text">Stillasittande</items>
                    <items id="description">Personen är spenderar mycket tid sittande, till exempel en kontorsarbetare som tränar lite eller inte alls.</items>
                </term_definitions>
                <term_definitions code="at0011">
                    <items id="text">Medelmåttig aktivitet</items>
                    <items id="description">Personen är medelmåttigt aktiv, till exempel en byggnadsarbetare eller en person som springer en timme varje dag.</items>
                </term_definitions>
                <term_definitions code="at0012">
                    <items id="text">Mycket aktiv</items>
                    <items id="description">Personen är väldigt aktiv, till exempel en person som rör dig mycket i sitt arbete eller en person som simmar två timmar varje dag.</items>
                </term_definitions>
                <term_definitions code="at0013">
                    <items id="text">Extremt aktiv</items>
                    <items id="description">Personen är extremt aktiv, till exempel en person som tävlingscyklar.</items>
                </term_definitions>
                <term_definitions code="at0014">
                    <items id="text">Tree</items>
                    <items id="description">@ internal @</items>
                </term_definitions>
                <term_definitions code="at0015">
                    <items id="text">Utökning</items>
                    <items id="description">Extra information som krävs för att fånga lokalt innehåll eller för att anpassa till andra referensmodeller.</items>
                    <items id="comment">Till exempel: lokal information som krävs</items>
                </term_definitions>
                <term_definitions code="at0016">
                    <items id="text">Senast uppdaterad</items>
                    <items id="description">Datumet när aktiviteten om fysisk aktivitet senast uppdaterades.</items>
                </term_definitions>
